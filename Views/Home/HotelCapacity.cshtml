@model IEnumerable<eHotels.Models.hotel_capacity>;



<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-sm-6 col-md-4 col-lg-3">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">@item.Hotel</h5>
                    <p class="card-text">@item.Capacity</p>
                </div>
            </div>
        </div>
    }
</div>
