@model eHotels.Models.Rentings

@{
    ViewData["Title"] = "Create";
}


<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" type="hidden">
                
                <input asp-for="RoomNumber" class="form-control" type="hidden"/>
                <span asp-validation-for="RoomNumber" class="text-danger" type="hidden"></span>
            </div>
            <div class="form-group" type="hidden">
                
                <input asp-for="Employee" class="form-control" type="hidden" />
                <span asp-validation-for="Employee" class="text-danger" type="hidden"></span>
            </div>
            <div class="form-group" type="hidden">
                
                <input asp-for="Customer" class="form-control" type="hidden" />
                <span asp-validation-for="Customer" class="text-danger" type="hidden"></span>
            </div>
            <div class="form-group" type="hidden">
                
                <input asp-for="Start" class="form-control" type="hidden" />
                <span asp-validation-for="Start" class="text-danger" type="hidden"></span>
            </div>
            <div class="form-group" type="hidden">
                
                <input asp-for="End" class="form-control" type="hidden" />
                <span asp-validation-for="End" class="text-danger" type="hidden"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Rent" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="IndexEmployee" asp-controller="Booking">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
